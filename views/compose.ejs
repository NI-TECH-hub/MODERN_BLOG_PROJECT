
<%- include('partials/header') -%> 


    <h1>Compose</h1> 
    <form action="/compose" method="post">
        <div class="mb-3">
            <label for="exampleFormControlInput1" class="form-label">Title</label>
            <input type="text" class="form-control" name="postTitle" id="exampleFormControlInput1">
          </div>
          <div class="mt-9">
            <label for="exampleFormControlTextarea1" class="form-label">Post</label>
            <textarea class="form-control" name="postBody" id="exampleFormControlTextarea1" rows="3"></textarea>
          </div>
          <div class="mt-3">
            <button type="submit" class="btn btn-primary" name="button">Publish</button>
          </div>
          
    </form>
    
<%- include('partials/footer') %> 